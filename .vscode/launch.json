{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "run Lambda function",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/lambda_function.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "AWS_LAMBDA_FUNCTION_NAME": "homework-scraper",
                "DYNAMODB_TABLE_NAME": "homework-items",
                "AWS_REGION": "us-east-1"
            },
            "args": []
        },
        {
            "name": "Debug Lambda Simulation",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/tests/test_all.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": []
        },
        {
            "name": "Debug Homework Agent",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": ["--scrape"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug Scheduler",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scheduler.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug Scraper Module",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scraper/runner.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug Agent: Interactive Mode",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run_agent.py",
            "console": "integratedTerminal",
            "args": [
                "--interactive"
            ],
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug Agent: Single Question (Today)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run_agent.py",
            "console": "integratedTerminal",
            "args": [
                "--question",
                "What's my homework for today?"
            ],
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug Agent: Dry Run",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run_agent.py",
            "console": "integratedTerminal",
            "args": [
                "--question",
                "List homework",
                "--dry-run"
            ],
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug Agent: Specific Date",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run_agent.py",
            "console": "integratedTerminal",
            "args": [
                "--date",
                "2025-11-13",
                "--question",
                "What homework do I have?"
            ],
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug Scrape All Historical",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": ["--scrape-all"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug Playwright Scraper - Daily",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/playwright-scraper/src/index.js",
            "cwd": "${workspaceFolder}/playwright-scraper",
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development",
                "PLAYWRIGHT_HEADLESS": "false"
            },
            "envFile": "${workspaceFolder}/playwright-scraper/.env",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": []
        },
        {
            "name": "Debug Playwright Scraper - Historical",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node-scraper/src/index.js",
            "cwd": "${workspaceFolder}/node-scraper",
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development",
                "PLAYWRIGHT_HEADLESS": "false"
            },
            "envFile": "${workspaceFolder}/node-scraper/.env",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["--historical"]
        },
        {
            "name": "Debug Playwright Scraper - No DynamoDB",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node-scraper/src/index.js",
            "cwd": "${workspaceFolder}/node-scraper",
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development",
                "PLAYWRIGHT_HEADLESS": "false"
            },
            "envFile": "${workspaceFolder}/node-scraper/.env",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["--no-dynamodb"]
        },
        {
            "name": "Debug Playwright Auth Only",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node-scraper/src/scraper/auth.js",
            "cwd": "${workspaceFolder}/node-scraper",
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development",
                "PLAYWRIGHT_HEADLESS": "false"
            },
            "envFile": "${workspaceFolder}/node-scraper/.env",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}
